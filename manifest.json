{
	"manifest_version": 2,

	"name": "Password Reminder",
	"description": "",
	"version": "1.0",

	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"options_page": "options.html",
	"permissions": [
		"webNavigation",
		"storage",
		"https://www.amazon.com/",
		"https://bitbucket.org/",
		"https://signin.ebay.com/",
		"https://www.facebook.com/",
		"https://github.com/",
		"https://accounts.google.com/",
		"https://www.ovh.com/",
		"https://twitter.com/"
	]
}
